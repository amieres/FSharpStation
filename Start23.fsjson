O |> Option.defaultValue 0 \n                                    return i * 12\n                                 }                                       \n                        test4 /= (CommArgCollection.getIntRm test3 |> ReaderM.map (( * ) 12))\n                        test5 /= (CommArgCollection.getIntRm test2 |> ReaderM.map (sprintf \"Value %d\"))\n                        test6 /= (CommArgCollection.getIntRm test2 |> ReaderM.map ( (=) 8))\n                        test7 /= (CommArgCollection.getIntRm test2 |> ReaderM.map float )\n                      ] :  (CommArg * ReaderM<obj,CommArgCollection<obj>,Message<obj>>) list\n    let args        = CommArgCollection pairs |> CommArgCollection.addPairs pairs |> CommArgCollection.addPairs  pairs\n    let produceRm   = args |> CommArgCollection.produceRm\n    let produceRm2  = args.CommArgs |> Seq.filter (fun (arg,_) -> arg.cargId <> test4.CommArg.cargId) |> CommArgCollection |> CommArgCollection.produceRm\n    let produce vRm = \n        ReaderM.run args vRm\n        |> Async.RunSynchronously \n        |> Result.map (fun (arg,v) -> sprintf \"%s %A\" arg.name v)\n        |> function Ok v -> v | Error m -> sprintf \"%A\" m\n    let produceCollection vRm = \n        ReaderM.run args vRm\n        |> Async.RunSynchronously \n        |> Result.map (Seq.map (fun (arg,v) -> sprintf \"%s %s\" arg.name v) >> String.concat \"\\n\" )\n        |> function Ok v -> v | Error m -> sprintf \"%A\" m\n    \n\n[\n    testCase \"CommArg test1\"             <|fun()-> CommArg.produce (CommArgCollection.findArgRm CommArg.test1) =! \"test1 5\"\n    testCase \"CommArg test2\"             <|fun()-> CommArg.produce (CommArgCollection.findArgRm CommArg.test2) =! \"test2 6\"\n    testCase \"CommArg test3\"             <|fun()-> CommArg.produce (CommArgCollection.findArgRm CommArg.test3) =! \"ErrorMsg \\\"argument not found: test3\\\"\"\n    testCase \"CommArgCollection produce\" <|fun()-> CommArg.produceCollection CommArg.produceRm2                =! \"test2 --T2 6\\ntest2 --T2 7\\ntest2 --T2 25\\ntest2 --T2 60\\ntest5 --T5 \\\"Value 6\\\"\\ntest6 --T6 false\\ntest7 --T7 6.0\\ntest2 --T2 6\\ntest2 --T2 7\\ntest2 --T2 25\\ntest2 --T2 60\\ntest5 --T5 \\\"Value 6\\\"\\ntest6 --T6 false\\ntest7 --T7 6.0\\ntest1 --T1 5\\ntest2 --T2 6\\ntest2 --T2 7\\ntest2 --T2 25\\ntest2 --T2 60\\ntest5 --T5 \\\"Value 6\\\"\\ntest6 --T6 false\\ntest7 --T7 6.0\"\n    testCase \"CommArgCollection produce\" <|fun()-> CommArg.produceCollection CommArg.produceRm                 =! \"ErrorMsg \\\"argument not found: test3\\\"\"\n] |> runTests \"fsiAgent\"\n\n","parent":{"$":0,"Item":"f66ab559-6876-4799-94fb-4a86a783054a"},"predecessors":[{"$":0,"Item":"c3be67ee-321f-4eb2-ba98-345957897fec"}],"id":{"$":0,"Item":"cdb289a4-b2cd-4488-be03-758125305ae0"},"expanded":true,"level":0,"levelCode":0,"properties":{}},{"name":"FsCompiler","content":"module FsCompiler =\n    open LibraryNoJS.FsCompiler\n  \n    let args =\n          [\n              fscDebug      /= \"full\"\n              fscDefine     /= \"DEBUG\"\n              fscDefine     /= \"TRACE\"\n              fscTarget     /= \"winexe\"\n              fscGenFSharp1 /= \"g\"\n              fscGenFSharp2 /= \"warn:3\"\n              fscGenFSharp2 /= \"warnaserror:76\"\n              fscGenFSharp2 /= \"vserrors\"\n              fscGenFSharp2 /= \"utf8output\"\n              fscGenFSharp2 /= \"fullpaths\"\n              fscGenFSharp2 /= \"flaterrors\"\n              fscGenFSharp2 /= \"subsystemversion:6.00\"\n              fscGenFSharp2 /= \"highentropyva+\"\n              fscGenFSharp2 /= \"optimize-\"\n              fscGenFSharp2 /= \"tailcalls-\"\n              fscSource     /= @\"Compiled\\Calculate primes\\Calculate primes.fs\"\n              fscOutput     /= @\"Compiled\\Calculate primes\\Calculate primes.exe\"\n              fscIOption    /= @\"C:\\Program Files (x86)\\Reference Assemblies\\Microsoft\\Framework\\.NETFramework\\v4.6.1\"\n              fscIOption    /= @\"C:\\Program 